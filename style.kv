<LoginWin>:
    name: 'login'
    MDTopAppBar:
        title: "Gerador QR - Mobile"
        right_action_items: [["dots-vertical", app.login]]
        left_action_items: [["qrcode-scan"]]
        type: "top"
        halign: "center"
        pos_hint: {'center_x': 0.5,'center_y': 0.96}
        elevation: 1
    MDTextField:
        id: entryServer
        hint_text: "Server"
        pos_hint: {'center_x': 0.5,'center_y': 0.6}
        helper_text: "10.10.1.10"
        size_hint_x: .8
    MDTextField:
        id: entryUser
        hint_text: "User"
        pos_hint: {'center_x': 0.5,'center_y': 0.5}
        size_hint_x: .8
    MDTextField:
        id: entryPwd 
        hint_text: "Senha"
        pos_hint: {'center_x': 0.5,'center_y': 0.4}
        password: True
        size_hint_x: .8
    MDSwitch:
        value: 'teste'
        pos_hint: {'center_x': 0.5,'center_y': 0.3}
    MDRaisedButton:
        text: "Login"
        pos_hint: {'center_x': 0.5,'center_y': 0.2}
        size_hint_x: .8
        font_size: 20
        on_press: app.login()
        
<Gerador>:
    name: 'gerador'
    MDFloatLayout:
        MDLabel:
            text: "gerador"